package quarter1;
/* Richard Paul
 * Sep 8, 2016
 * String Operations
 */
public class StringOps {

	public static void main(String[] args) {
		String first = "Ian";
		String last = "jkldsafjksdfkjdsfakjljkldsfjhdsafhkjlfdPark";
		String name = first + " " + last;
		System.out.println(name.substring(name.length()- 4));
	}

}
